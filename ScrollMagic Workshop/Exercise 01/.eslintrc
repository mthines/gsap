{
  "root": true,
  "extends": [],
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true,
    "mocha": true
  },
  "rules": {
    "max-len": [1, {"code": 120}],
    "class-methods-use-this": 0,
    "quotes": 0,
    "no-console": 1,
    "no-debugger": 1,
    "no-var": 1,
    "semi": [1, "always"],
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "no-underscore-dangle": 0,
    "no-alert": 0,
    "no-lone-blocks": 0,
    "no-plusplus": 0,
    "jsx-quotes": 1,
    "prefer-stateless-function": 0,
    "no-unused-vars": 1
  },
  "globals": {
    "global": false,
    "process": false,
    "require": true,
    "Clerk": false
  }
}
